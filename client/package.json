{
  "name": "student-experience",
  "version": "0.0.1",
  "description": "Dumm description",
  "repository": {
    "type": "git",
    "url": "git://github.com/pufe/student-experience.git"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run watch-css\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build-css": "node-sass --include-path stylesheet/scss/ stylesheet/scss/* stylesheet/main.css",
    "watch-css": "nodemon -e stylesheet/scss/* -x \"npm run build-css\""
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
